      "settings": [
        {
          "id": "mtpupil_superTopics_notice",
          "name": "签到通知",
          "val": true,
          "type": "boolean",
          "desc": "关闭后可免打扰（参数失效、签到失败等情况不受影响，仅签到成功的时候不通知）"
        },
        {
          "id": "mtpupil_superTopics_bat",
